
public class Main
{
	public static void main(String[] args) {
		int[] a={20,18,16,17,14};
		int ele,j;
		for(int i=1;i<5;i++){
		    ele=a[i];
		    j=i-1;
		    while(j>=0&&a[j]>ele){
		        a[j+1]=a[j];
		        j--;
		    }
		    a[j+1]=ele;
		}
		System.out.print("sorting :");
		for(int i=0;i<5;i++){
		    System.out.print(a[i]+" ");
		}
	}
}

